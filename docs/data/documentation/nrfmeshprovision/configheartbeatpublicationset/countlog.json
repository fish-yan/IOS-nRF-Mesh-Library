{"sections":[],"hierarchy":{"paths":[["doc:\/\/nRFMeshProvision\/documentation\/nRFMeshProvision","doc:\/\/nRFMeshProvision\/documentation\/nRFMeshProvision\/ConfigHeartbeatPublicationSet"]]},"metadata":{"modules":[{"name":"nRFMeshProvision"}],"role":"symbol","symbolKind":"property","fragments":[{"text":"let","kind":"keyword"},{"kind":"text","text":" "},{"text":"countLog","kind":"identifier"},{"kind":"text","text":": "},{"preciseIdentifier":"s:s5UInt8V","kind":"typeIdentifier","text":"UInt8"}],"title":"countLog","externalID":"s:16nRFMeshProvision29ConfigHeartbeatPublicationSetV8countLogs5UInt8Vvp","roleHeading":"Instance Property"},"abstract":[{"text":"Number of Heartbeat messages remaining to be sent.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/nrfmeshprovision\/configheartbeatpublicationset\/countlog"]}],"schemaVersion":{"patch":0,"minor":3,"major":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"countLog"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:s5UInt8V","text":"UInt8"}],"languages":["swift"],"platforms":["macOS"]}]},{"content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"text":"Possible values:","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"0x00 - Periodic Heartbeat messages are not published."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"0x01 - 0x11 - Number of Heartbeat messages, 2^(n-1), that remain to be sent."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"0xFF - Periodic Heartbeat messages are published indefinitely."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"Other values are prohibited.","type":"text"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The Heartbeat Publication Count Log is a representation of the Heartbeat Publication"},{"text":" ","type":"text"},{"text":"Count state value. The Heartbeat Publication Count Log and Heartbeat Publication Count","type":"text"},{"type":"text","text":" "},{"type":"text","text":"with the value 0x00 and 0x0000 are equivalent. The Heartbeat Publication Count Log"},{"text":" ","type":"text"},{"text":"value of 0xFF is equivalent to the Heartbeat Publication count value of 0xFFFF.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"The Heartbeat Publication Count Log value between 0x01 and 0x11 shall represent that"},{"text":" ","type":"text"},{"type":"text","text":"smallest integer n where 2^(n-1) is greater than or equal to the Heartbeat Publication"},{"type":"text","text":" "},{"type":"text","text":"Count value. For example, if the Heartbeat Publication Count value is 0x0579, then the"},{"type":"text","text":" "},{"text":"Heartbeat Publication Count Log value would be 0x0C.","type":"text"}]}],"kind":"content"}],"kind":"symbol","identifier":{"interfaceLanguage":"swift","url":"doc:\/\/nRFMeshProvision\/documentation\/nRFMeshProvision\/ConfigHeartbeatPublicationSet\/countLog"},"references":{"doc://nRFMeshProvision/documentation/nRFMeshProvision/ConfigHeartbeatPublicationSet/countLog":{"title":"countLog","fragments":[{"text":"let","kind":"keyword"},{"kind":"text","text":" "},{"text":"countLog","kind":"identifier"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UInt8","preciseIdentifier":"s:s5UInt8V"}],"identifier":"doc:\/\/nRFMeshProvision\/documentation\/nRFMeshProvision\/ConfigHeartbeatPublicationSet\/countLog","role":"symbol","url":"\/documentation\/nrfmeshprovision\/configheartbeatpublicationset\/countlog","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"Number of Heartbeat messages remaining to be sent."}]},"doc://nRFMeshProvision/documentation/nRFMeshProvision/ConfigHeartbeatPublicationSet":{"title":"ConfigHeartbeatPublicationSet","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"ConfigHeartbeatPublicationSet"}],"identifier":"doc:\/\/nRFMeshProvision\/documentation\/nRFMeshProvision\/ConfigHeartbeatPublicationSet","role":"symbol","url":"\/documentation\/nrfmeshprovision\/configheartbeatpublicationset","abstract":[],"type":"topic","kind":"symbol","navigatorTitle":[{"text":"ConfigHeartbeatPublicationSet","kind":"identifier"}]},"doc://nRFMeshProvision/documentation/nRFMeshProvision":{"kind":"symbol","type":"topic","role":"collection","title":"nRFMeshProvision","url":"\/documentation\/nrfmeshprovision","identifier":"doc:\/\/nRFMeshProvision\/documentation\/nRFMeshProvision","abstract":[{"text":"Provision, configure and control Bluetooth mesh devices with nRF Mesh library.","type":"text"}]}}}